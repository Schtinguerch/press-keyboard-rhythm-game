<Window x:Class="Press.GameClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:gameComp="clr-namespace:Press.GameComponents.Controls.TappingCircles;assembly=Press.GameComponents"
        xmlns:local="clr-namespace:Press.GameClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1600"
        KeyDown="MainWindowOnKeyDown">
    <Grid>
        <Canvas 
            x:Name="GameSceneCanvas"
            Background="#161616">
            <MediaElement 
                x:Name="GameMusicElement" 
                Opacity="0" 
                Width="0" 
                Height="0"/>

            <TextBlock
                x:Name="Test"
                FontSize="36"
                Foreground="White">
            </TextBlock>
        </Canvas>
    </Grid>
</Window>
